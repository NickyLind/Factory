@{
  Layout = "_Layout";
}
@using Factory.Models;
@model List<Factory.Models.Engineer>;

<h1>Welcome to the Factory</h1>
<h3>Engineers:</h3>
<ul>
@foreach (Engineer engineer in ViewBag.Engineers)
{
  <li>@engineer.Name</li>
}
</ul>
<h3>Machines:</h3>
<ul>
@foreach (Machine machine in ViewBag.Machines)
{
  <li>@machine.Name</li>
}
</ul>

<p>@Html.ActionLink("Engineers Home", "Index", "Engineers")</p>
<p>@Html.ActionLink("Machines Home", "Index", "Machines")</p>